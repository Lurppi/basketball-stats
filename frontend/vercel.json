{
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "outputDirectory": "dist",  // Stelle sicher, dass das Output-Verzeichnis korrekt ist
    "framework": "vite",        // Entferne diesen Eintrag, falls er Probleme verursacht
    "installCommand": "yarn install",
    "buildCommand": "yarn build",
    "cleanUrls": true
  }
  